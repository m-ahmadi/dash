    "C": {
        "html": "htmlbilder ./src/html/ -o ./dist/index.html -t main.handlebars -e .htm",
        "temp": "handlebars ./src/template/ -f ./src/js/templates.js -e hbs -m",
        "js": "node node_modules/requirejs/bin/r.js -o ./rconfig.js && babel ./src/js/ -o ./dist/js/app.min.js -s --minified --no-comments",
        "sass": "sass ./src/sass/style.scss:./dist/css/style.min.css --style compressed --sourcemap=none",
        "w": {
            "html": "htmlbilder ./src/html/ -o ./dist/index.html -t main.handlebars -e .htm -w",
            "sass": "sass ./src/sass/style.scss:./dist/css/style.min.css --style compressed --sourcemap=none --watch",
            "js": "node node_modules/requirejs/bin/r.js -o ./rconfig.js && babel ./src/js/ -o ./dist/js/app.min.js -s --minified --no-comments -w"
        },
        "all": "htmlbilder ./src/html/ -o ./dist/index.html -t main.handlebars -e .htm && sass ./src/sass/style.scss:./dist/css/style.min.css --style compressed --sourcemap=none && handlebars ./src/template/ -f ./src/js/templates.js -e hbs -m && node node_modules/requirejs/bin/r.js -o ./rconfig.js && babel ./src/js/ -o ./dist/js/app.min.js -s --minified --no-comments"
    }

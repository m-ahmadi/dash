<tr data-id="{{id}}">
	<td title="{{name}}">{{name}}</td>
	<td>
		<select class="uk-select uk-form-small" data-el="select" data-els="toDisable">
			{{#each units}}
				<option value="{{this.name}}">{{this.name}}</option>
			{{/each}}
		</select>
	</td>
	<td>
		<input type="text" data-el="colorpick" />
	</td>
	<td>
		<button type="button" class="uk-button uk-button-danger uk-button-small small-btn" data-el="remove" data-els="toDisable">
			<span uk-icon="icon: close; ratio: 0.7" class="uk-icon"></span>
		</button>
	</td>
</tr>
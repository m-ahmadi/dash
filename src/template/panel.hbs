<div class="
		panel
		uk-card
		uk-card-default
	{{#if expand}}
		uk-width-1-1@s
		uk-width-1-1@m
		uk-width-1-2@l
		uk-width-1-2@xl
	{{/if}}"
	data-el="panel" data-id="{{id}}">
	<div class="panel-wrap">
	<div class="uk-card-header">
		<div class="uk-float-left">
			<span class="uk-card-title uk-text-muted uk-text-small uk-text-bold">
				<i class="uk-sortable-handle" uk-icon="icon: move; ratio: 1"></i><span data-el="title"> {{title}}</span>
				<span class="uk-margin-small-left" data-el="spinnerParent"></span>
			</span>
		</div>
		<span class="uk-text-small uk-text-meta">{{range}}</span>
		<div class="uk-float-right">
			<div class="uk-inline">
				<button class="dropdown uk-button uk-button-default uk-button-small" type="button">
					<span uk-icon="icon: settings; ratio: 1"></span>
				</button>
				<div uk-dropdown="delay-hide: 50;">
					<ul class="uk-list uk-text-left" data-el="menus">
						{{#if expand}}
							<li data-resize><a class="uk-button uk-button-text uk-text-small uk-text-meta" data-menu data-action="1"><i uk-icon="icon: shrink; ratio: 0.8;"></i> Shrink</a></li>
						{{else}}
							<li data-resize><a class="uk-button uk-button-text uk-text-small uk-text-meta" data-menu data-action="0"><i uk-icon="icon: expand; ratio: 0.8;"></i> Expand</a></li>
						{{/if}}
						<li><a class="uk-button uk-button-text uk-text-small uk-text-meta" data-el="edit"><i uk-icon="icon: file-edit; ratio: 0.8;"></i> Edit</a></li>
						<li><a class="uk-button uk-button-text uk-text-small uk-text-meta" data-el="refresh"><i uk-icon="icon: refresh; ratio: 0.8;"></i> Refresh</a></li>
						<li><a class="uk-button uk-button-text uk-text-small uk-text-meta" data-el="save"><i uk-icon="icon:  file-edit; ratio: 0.8;"></i> Save</a></li>
						<li><a class="uk-button uk-button-text uk-text-small uk-text-meta" data-el="remove"><i uk-icon="icon: close; ratio: 0.8;"></i> Delete</a></li>
					</ul>
				</div>
			</div>
		</div>
	</div>
	<div class="uk-card-body" data-el="body">
		
	</div>
	</div>
</div>
<div class="uk-overflow-auto uk-padding-small stat-table">
	<table class="uk-table uk-table-divider uk-table-hover uk-table-small uk-table-striped">
		<caption class="uk-text-center uk-margin-small-bottom">{{title}}</caption><!-- uk-text-bold -->
		<tr>
			<th class="uk-text-middle">Logical Links</td>
			<td><img src="images/links.png" width="40" height="40" /></td>
			<td class="uk-text-middle uk-text-bold" data-el="total_link_count"></td>
		</tr>
		<tr>
			<th class="uk-text-middle">Links with SLA Violation</th>
			<td><img src="images/sla_violation.png" width="40" height="40" /></td>
			<td class="uk-text-middle uk-text-bold" data-el="total_violation"></td>
		</tr>
		<tr>
			<th class="uk-text-middle">Links with <i class="uk-text-primary">packet_loss_ne</i> Violation</th>
			<td><img src="images/packet_loss.png" width="40" height="40" /></td>
			<td class="uk-text-middle uk-text-bold" data-el="total_packet_losss_ne_violation"></td>
		</tr>
		<tr>
			<th class="uk-text-middle">Links with <i class="uk-text-primary">two_way_delay_max</i> Violation</th>
			<td><img src="images/two_way_delay.png" width="40" height="40" /></td>
			<td class="uk-text-middle uk-text-bold" data-el="total_two_way_delay_max_violation"></td>
		</tr>
		<tr>
			<th class="uk-text-middle">Links with <i class="uk-text-primary">two_way_dv_max</i> Violation</th>
			<td><img src="images/two_way_dv.png" width="40" height="40" /></td>
			<td class="uk-text-middle uk-text-bold" data-el="total_two_way_dv_max_violation"></td>
		</tr>
	</table>
</div>
